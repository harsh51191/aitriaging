{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "api/triage.js": {
      "maxDuration": 60
    },
    "api/health.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key",
    "CLAUDE_API_KEY": "@claude_api_key"
  }
}
